    "import collections\n",
    "import sys\n",
    "\n",
    "n = int(sys.stdin.readline().rstrip())\n",
    "d = collections.deque([x for x in range(1,n+1)])\n",
    "l = len(d)\n",
    "while l != 1:\n",
    "    d.popleft()\n",
    "    temp = d.popleft()\n",
    "    d.append(temp)\n",
    "    l = len(d)\n",
    "print(d[0])"
